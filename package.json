{
  "name": "create-async-generator",
  "version": "2.0.0",
  "description": "Create an async generator out of any value or combination of values and pipe async generators into each other",
  "exports": {
    ".": "./src/index.js",
    "./*": "./src/*",
    "./operators": "./src/operators/index.js"
  },
  "type": "module",
  "scripts": {
    "format": "prettier --check \"**/*.js\"",
    "format:fix": "prettier --write \"**/*.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/handijk/create-async-generator.git"
  },
  "keywords": [
    "async",
    "generator",
    "iterable",
    "iterator",
    "javascript",
    "combine",
    "pipe",
    "operators"
  ],
  "author": "Han Dijk <info@handijk.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/handijk/create-async-generator/issues"
  },
  "homepage": "https://github.com/handijk/create-async-generator#readme",
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.2",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-floating-promise": "^1.0.2",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "prettier": "^3.1.0",
    "vitest": "^1.0.2"
  },
  "optionalDependencies": {
    "async-iterators-combine": "^2.0.0"
  }
}
